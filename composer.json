{
  "name": "skaut/skautis-integration",
  "description": "A WordPress plugin for integration with SkautIS",
  "type": "wordpress-plugin",
    "keywords": [
        "WordPress",
        "WordPress plugin",
        "skaut",
        "SkautIS"
    ],
  "homepage": "https://wordpress.org/plugins/skautis-integration",
  "readme": "README.md",
  "license": "proprietary",
  "authors": [
    {
      "name": "David Odehnal",
      "email": "info@davidodehnal.cz"
    },
    {
        "name": "Marek Dědič",
        "email": "developer@dedic.eu"
    }
  ],
  "support": {
    "issues": "https://github.com/skaut/skautis-integration/issues",
    "source": "https://github.com/skaut/skautis-integration"
  },
  "config": {
    "optimize-autoloader": true,
    "classmap-authoritative": true,
    "notify-on-install": false,
    "platform": {
      "php": "7.1"
    }
  },
  "require": {
    "php": "^7.0",
    "pimple/pimple": "~3.0",
    "skautis/skautis": "~2.1"
  },
  "autoload": {
    "psr-4": {
      "SkautisIntegration\\": "src/"
    },
    "files": [
      "plugin/globalFunctions.php"
    ]
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.6",
    "phan/phan": "^2.7",
    "phpmd/phpmd": "^2.10",
    "phpcompatibility/phpcompatibility-wp": "^2.1",
    "wp-coding-standards/wpcs": "^2.3"
  }
}
