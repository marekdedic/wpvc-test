{
  "name": "skautis/WordPressIntegration",
  "type": "library",
  "description": "WordPress integration with skautIS",
  "license": "proprietary",
  "authors": [
    {
      "name": "David Odehnal",
      "email": "info@davidodehnal.cz"
    }
  ],
  "config": {
    "optimize-autoloader": true,
    "classmap-authoritative": true,
    "notify-on-install": false,
    "platform": {
      "php": "7.1"
    }
  },
  "require": {
    "php": "^7.0",
    "pimple/pimple": "~3.0",
    "skautis/skautis": "~2.1"
